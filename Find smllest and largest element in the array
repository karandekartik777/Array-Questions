def largest_smallest(arr):
    smallest  = arr[0]
    largest = arr[0]
    
    for i in range(len(arr)):
        if arr[i]>largest:
            largest = arr[i]
        if arr[i]<smallest:
            smallest=arr[i]
    return smallest, largest
    
arr=[1,2,3,4,5,6,7,8,9]
print(largest_smallest(arr))
