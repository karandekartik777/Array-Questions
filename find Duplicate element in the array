def removeDups(arr):
    result = []
    
    for i in range(len(arr)):
        for j in range(i+1,len(arr)):
            if arr[i] == arr[j]:
                result.append(arr[i])
    return result
arr = [ 1, 2, 3, 4 ,3,4,5,5]
print(removeDups(arr))



